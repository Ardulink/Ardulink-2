{
  "extends": ["config:recommended"],
  "pruneStaleBranches": true,
  "docker-compose": {
    "fileMatch": [
      "^deploy-dist/tests/docker-compose\\.yml$"
    ]
  },
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": false
    },
    {
      "matchPackageNames": ["org.junit:junit-bom"],
      "allowedVersions": "<6.0.0"
    }
  ]
}

